{
    "name": "luqta/laravel-passport-mongodb",
    "description": "Laravel Passport provides OAuth2 server support to Laravel.",
    "keywords": [
        "laravel",
        "passport",
        "oauth"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "luqta",
            "email": "dialyy@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "firebase/php-jwt": "~5.0",
        "guzzlehttp/guzzle": "~6.5",
        "illuminate/auth": "^5.8.9",
        "illuminate/console": "~6.0",
        "illuminate/container": "~6.0",
        "illuminate/contracts": "~6.0",
        "illuminate/database": "~6.0",
        "illuminate/encryption": "~6.0",
        "illuminate/http": "~6.0",
        "illuminate/support": "~6.0",
        "league/oauth2-server": "^6.1",
        "phpseclib/phpseclib": "^2.0",
        "symfony/psr-http-message-bridge": "~1.3",
        "zendframework/zend-diactoros": "~1.8"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "psr-4": {
            "Luqta\\LaravelPassportMongoDB\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        },
        "laravel": {
            "providers": [
                "Luqta\\LaravelPassportMongoDB\\PassportServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}